{
  "name": "meupaozin",
  "version": "0.0.1",
  "description": "API para gerenciamento de pedidos de p√£es",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:up": "command docker-compose up -d",
    "docker:down": "command docker-compose down",
    "docker:restart": "command docker-compose restart",
    "docker:logs": "command docker-compose logs -f",
    "docker:status": "command docker-compose ps",
    "docker:build": "command docker-compose build",
    "docker:clean": "command docker-compose down -v --remove-orphans",
    "docker:db": "command docker-compose exec postgres psql -U postgres -d pedidos_db",
    "docker:seed": "node scripts/seed-data.js",
    "docker:full-setup": "npm run docker:up && sleep 10 && npm run docker:seed",
    "kafka:init-topics": "./scripts/init-kafka-topics.sh",
    "kafka:logs": "command docker-compose logs -f kafka",
    "kafka:ui": "open http://localhost:8080",
    "kafka:test": "node scripts/test-kafka-events.js",
    "setup:complete": "npm run docker:up && sleep 30 && npm run kafka:init-topics && sleep 10 && npm run docker:seed",
    "datadog:up": "docker-compose -f docker-compose.datadog.yml --profile datadog up -d",
    "datadog:down": "docker-compose -f docker-compose.datadog.yml --profile datadog down",
    "datadog:logs": "docker-compose -f docker-compose.datadog.yml --profile datadog logs -f",
    "datadog:status": "docker-compose -f docker-compose.datadog.yml --profile datadog ps",
    "setup:datadog": "npm run datadog:up && sleep 10 && npm run docker:up",
    "setup:full": "npm run setup:datadog && sleep 30 && npm run kafka:init-topics && sleep 10 && npm run docker:seed"
  },
  "dependencies": {
    "@datadog/browser-logs": "^6.17.0",
    "@datadog/browser-rum": "^6.17.0",
    "@nestjs/common": "^10.4.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.4.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/microservices": "^10.4.0",
    "@nestjs/platform-express": "^10.4.0",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dd-trace": "^5.62.0",
    "kafkajs": "^2.2.4",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^10.4.0",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.4.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "axios": "^1.6.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
